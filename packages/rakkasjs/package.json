{
	"name": "rakkasjs",
	"version": "0.5.0-next.3",
	"description": "Lightning fast Next.js alternative powered by Vite",
	"scripts": {
		"prepublishOnly": "pnpm build",
		"build": "rimraf dist && cross-env NODE_ENV=production rollup --config rollup.config.js",
		"dev": "rollup --watch --config rollup.config.js",
		"precommit": "lint-staged --no-stash",
		"check": "run-p typecheck lint:ts test",
		"typecheck": "tsc -p tsconfig.json --noEmit",
		"lint:ts": "eslint --max-warnings=0 src",
		"test": "jest"
	},
	"author": "Fatih Ayg√ºn <cyco130@gmail.com>",
	"license": "MIT",
	"repository": "https://github.com/rakkasjs/rakkasjs",
	"homepage": "https://rakkasjs.org",
	"keywords": [
		"react",
		"vite",
		"nextjs",
		"ssr"
	],
	"files": [
		"dist/**/*",
		"*.d.ts"
	],
	"exports": {
		".": "./dist/index.js",
		"./server": "./dist/server.js",
		"./client": "./dist/client.js"
	},
	"dependencies": {
		"@types/react": "^17.0.37",
		"vite": "2.7.2"
	},
	"peerDependencies": {
		"react": "^17",
		"react-dom": "^17",
		"react-helmet-async": "^1"
	},
	"devDependencies": {
		"@babel/core": "^7.16.5",
		"@babel/plugin-transform-runtime": "^7.16.5",
		"@babel/preset-env": "^7.16.5",
		"@babel/preset-react": "^7.16.5",
		"@babel/preset-typescript": "^7.16.5",
		"@rollup/plugin-commonjs": "^21.0.1",
		"@rollup/plugin-node-resolve": "^13.1.1",
		"@testing-library/jest-dom": "^5.16.1",
		"@testing-library/react": "^12.1.2",
		"@types/jest": "^27.0.3",
		"@types/react-dom": "^17.0.11",
		"@types/testing-library__jest-dom": "^5.14.2",
		"@typescript-eslint/eslint-plugin": "^5.7.0",
		"@typescript-eslint/parser": "^5.7.0",
		"babel-plugin-transform-vite-meta-env": "^1.0.3",
		"core-js": "^3.19.3",
		"cross-env": "^7.0.3",
		"devalue": "^2.0.1",
		"eslint": "^8.4.1",
		"eslint-plugin-import": "^2.25.3",
		"eslint-plugin-only-warn": "^1.0.3",
		"eslint-plugin-react": "^7.27.1",
		"eslint-plugin-react-hooks": "^4.3.0",
		"jest": "^27.4.5",
		"lint-staged": "^12.1.2",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.5.1",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-helmet-async": "^1.1.2",
		"rimraf": "^3.0.2",
		"rollup": "^2.61.1",
		"rollup-plugin-terser": "^7.0.2",
		"rollup-plugin-ts": "^2.0.4",
		"typescript": "^4.5.4"
	}
}
