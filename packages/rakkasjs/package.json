		"history": "^5.0.0",
		"md5": "^2.3.0",
		"react-helmet-async": "^1.0.9",
		"vite": "^2.3.8"
	},
	"peerDependencies": {
		"react": "^17",
		"react-dom": "^17"
	},
	"devDependencies": {
		"@babel/core": "^7.14.6",
		"@babel/preset-env": "^7.14.7",
