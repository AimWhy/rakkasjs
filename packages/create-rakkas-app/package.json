{
	"name": "create-rakkas-app",
	"version": "0.6.2-next.7",
	"type": "module",
	"files": [
		"dist",
		"templates"
	],
	"bin": {
		"create-rakkas-app": "./index.js"
	},
	"scripts": {
		"build": "tsup && tsx scripts/create-templates.ts",
		"dev": "tsup --watch",
		"prepack": "rm -rf dist && pnpm build",
		"test": "pnpm test:typecheck && pnpm test:lint && pnpm test:unit",
		"test:unit": "vitest run",
		"test:typecheck": "tsc -p tsconfig.json --noEmit",
		"test:lint": "eslint . --max-warnings 0 --ignore-pattern dist --ignore-pattern templates"
	},
	"description": "Generate Rakkas application boilerplate",
	"license": "MIT",
	"repository": "https://github.com/rakkasjs/rakkasjs",
	"keywords": [
		"react",
		"vite",
		"rakkas"
	],
	"devDependencies": {
		"@cyco130/eslint-config": "^2.1.1",
		"@types/inquirer": "^9.0.2",
		"@types/mkdirp": "^1.0.2",
		"@types/node": "^18.8.3",
		"@types/rimraf": "^3.0.2",
		"detype": "^0.6.0",
		"eslint": "^8.25.0",
		"prettier": "^2.7.1",
		"publint": "^0.1.1",
		"tsup": "^6.2.3",
		"tsx": "^3.10.1",
		"typescript": "^4.8.4",
		"vitest": "^0.24.0"
	},
	"dependencies": {
		"cac": "^6.7.14",
		"cpr": "^3.0.1",
		"inquirer": "^9.1.3",
		"mkdirp": "^1.0.4",
		"picocolors": "^1.0.0",
		"rimraf": "^3.0.2"
	}
}
