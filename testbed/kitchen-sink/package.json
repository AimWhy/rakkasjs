{
	"name": "@rakkasjs/testbed-kitchen-sink",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite dev -l silent --port 3000",
		"build": "rakkas build -l silent",
		"start": "node dist/server",
		"ci": "vitest run",
		"ci:local": "TEST_HOST=http://localhost:3000 vitest run",
		"test": "pnpm test:typecheck && pnpm test:lint",
		"test:typecheck": "tsc -p tsconfig.json --noEmit",
		"test:lint": "eslint . --max-warnings 0 --ignore-pattern dist"
	},
	"devDependencies": {
		"@cyco130/eslint-config": "^2.0.0",
		"eslint": "^8.17.0",
		"kill-port": "^2.0.0",
		"node-fetch": "^3.2.2",
		"puppeteer": "^14.3.0",
		"rakkasjs": "workspace:*",
		"typescript": "^4.7.3",
		"vite": "3.0.0-alpha.9"
	},
	"dependencies": {
		"@types/react": "^18.0.12",
		"@types/react-dom": "^18.0.5",
		"react": "^18.1.0",
		"react-dom": "^18.1.0"
	}
}
